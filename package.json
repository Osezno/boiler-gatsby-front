{
  "name": "pixel-ground",
  "version": "1.0.0",
  "private": true,
  "author": "OSEZNO",
  "description": "pixel-ground",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "dev": "env-cmd -f .env gatsby develop -p 8000 -H 0.0.0.0",
    "dev:qa": "env-cmd -f .env.qa gatsby develop -H 0.0.0.0",
    "dev:prod": "env-cmd -f .env.prod gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "start": "gatsby serve -H 0.0.0.0 -p 8000",
    "copy-static-queries": "cp -r ./public/page-data/sq/d ./public/static",
    "storybook": "yarn copy-static-queries && start-storybook -s public -p 8080 --ci",
    "storybook:build": "yarn copy-static-queries && build-storybook -s public -o docs",
    "storybook:start": "serve docs -l 8080",
    "test": "jest --coverage",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless",
    "test:e2e": "is-ci test:e2e:run test:e2e:open",
    "pretest:e2e:run": "yarn build",
    "test:e2e:run": "server-test 8000 cy:run",
    "test:e2e:open": "start-server-and-test dev 8000 cy:open",
    "coverage": "open ./coverage/lcov-report/index.html",
    "stats": "open ./public/report.html",
    "lint": "eslint . --ext .tsx,.ts,.mdx,.js --fix",
    "prettier": "prettier --write",
    "format": "prettier . --write --ignore-path .gitignore",
    "type-check": "tsc --noEmit",
    "validate": "yarn lint && yarn type-check",
    "refreshVSToken": "vsts-npm-auth -config .npmrc",
    "clean": "gatsby clean",
    "deploy": "cross-env AWS_ACCESS_KEY_ID=${AWS_ACCESS_KEY_ID} AWS_SECRET_ACCESS_KEY=${AWS_SECRET_ACCESS_KEY} gatsby-plugin-s3 deploy --yes"
  },
  "dependencies": {
    "@emotion/core": "@10.1.1",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "axios": "^0.21.1",
    "cross-fetch": "^3.0.6",
    "dotenv": "^10.0.0",
    "env-cmd": "^10.1.0",
    "gatsby": "^5.10.0",
    "gatsby-plugin-emotion": "^4.3.13",
    "gatsby-alias-imports": "^1.0.4",
    "gatsby-plugin-react-svg":"^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.2.1"
  },
  "devDependencies": {
    "@emotion/jest": "^11.1.0",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "typescript": "^5.0.4"
  }
}
